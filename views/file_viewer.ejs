<link rel="stylesheet" type="text/css" href="../assets/css/upload-styles.css">
<div class="nav-bar" style="position: fixed;">
    <a href="/" class="back-iron">
        <i class="fa-solid fa-circle-arrow-left" style="color: white;"></i>
    </a>
    <div class="title">
        CSVUpload
    </div>
    <div class="sub-title">
        Viewer
    </div>
    <div class="file-name">
        <%= fileName %>
    </div>
</div>
<div class="search-bar">
    <form id="searchForm">
        <div class="search-box">
            <input type="text" id="search-input" placeholder="Search data...">
            <button id="go" class="search-btn" type="button">Go</button>
        </div>
    </form>
    <div>
        <p id="total-rows">Total Rows: <%= length %></p>
    </div>
</div>
<div class="result-aria">
    <table>
        <thead>
            <tr>
                <% for(let h of head) { %>
                    <th><%= h %></th>
                <% } %>
            </tr>
        </thead>
        <tbody>
            <% for(let row of data) { %>
                <tr>
                    <% for(let key of Object.keys(row)) { %>
                        <td><%= row[key] %></td>
                   <% } %>                             
                </tr>
            <% } %>
        </tbody>
    </table>
</div>